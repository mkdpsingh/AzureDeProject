{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "MksAzureDeOlistDf"
		},
		"AzureDataLakeStorageOlist_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorageOlist'"
		},
		"MySqlOlistProject_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'MySqlOlistProject'"
		},
		"AzureDataLakeStorageOlist_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://mksoliststorageaccount.dfs.core.windows.net/"
		},
		"HttpGitHubLink_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://raw.githubusercontent.com/mkdpsingh/"
		},
		"JsonFileHttpLink_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://raw.githubusercontent.com/mkdpsingh/AzureDeProject/refs/heads/main/Project-OlistBrazilianE-Commerce/ForEachInput.json"
		},
		"MySqlOlistProject_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "ypybi.h.filess.io"
		},
		"MySqlOlistProject_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "olistProject_youlabelbe"
		},
		"MySqlOlistProject_properties_typeProperties_username": {
			"type": "string",
			"defaultValue": "olistProject_youlabelbe"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "MksAzureDeOlistDf_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDataLakeStorageOlist_accountKey": {
						"value": "[parameters('AzureDataLakeStorageOlist_accountKey')]"
					},
					"MySqlOlistProject_password": {
						"value": "[parameters('MySqlOlistProject_password')]"
					},
					"AzureDataLakeStorageOlist_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorageOlist_properties_typeProperties_url')]"
					},
					"HttpGitHubLink_properties_typeProperties_url": {
						"value": "[parameters('HttpGitHubLink_properties_typeProperties_url')]"
					},
					"JsonFileHttpLink_properties_typeProperties_url": {
						"value": "[parameters('JsonFileHttpLink_properties_typeProperties_url')]"
					},
					"MySqlOlistProject_properties_typeProperties_server": {
						"value": "[parameters('MySqlOlistProject_properties_typeProperties_server')]"
					},
					"MySqlOlistProject_properties_typeProperties_database": {
						"value": "[parameters('MySqlOlistProject_properties_typeProperties_database')]"
					},
					"MySqlOlistProject_properties_typeProperties_username": {
						"value": "[parameters('MySqlOlistProject_properties_typeProperties_username')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}